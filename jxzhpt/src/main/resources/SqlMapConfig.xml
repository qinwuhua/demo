<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"mybatis-3-config.dtd">
<configuration>
  <properties resource="dao.properties"/>
  <typeAliases>
     <!-- BONECP数据库连接池 -->
     <typeAlias type="com.hdsx.dao.util.BoneCPFactory" alias="BONECP"/>
     <typeAlias type="com.hdsx.jxzhpt.xtgl.bean.TreeNode" alias="treenode"/>
     <typeAlias type="com.hdsx.jxzhpt.xtgl.bean.Master" alias="master"/>
     <typeAlias type="com.hdsx.jxzhpt.xtgl.bean.Unit" alias="unit"/>
     <typeAlias type="com.hdsx.jxzhpt.xtgl.bean.Param" alias="param"/>
     <!-- qwh begin -->
     <typeAlias type="com.hdsx.jxzhpt.gcgl.bean.Gcglwqgz" alias="gcglwqgz"/>
     <!-- qwh end -->
     <!-- lhp -->
     <typeAlias type="com.hdsx.jxzhpt.lwxm.xmjck.bean.Jckabgc" alias="jckabgc"/>
     <!-- lhp -->
     <!-- rh -->
     <typeAlias type="com.hdsx.jxzhpt.jhgl.bean.Plan_gcgj" alias="plan_gcgj"/>
     <typeAlias type="com.hdsx.jxzhpt.jhgl.bean.Plan_lx_gcgj" alias="plan_lx_gcgj"/>
     <!-- rh -->
  </typeAliases>
  <environments default="jdbc">
     <environment id="jdbc">
       <transactionManager type="JDBC"/>
       <!-- BONECP连接池，推荐在开发时使用 -->
       <dataSource type="BONECP">
			<property name="driverClass" value="${driverClass}" />
			<property name="jdbcUrl" value="${jdbcUrl}" />
			<property name="username" value="${username}" />
			<property name="password" value="${password}" />
			<property name="idleConnectionTestPeriod" value="60" />
			<property name="idleMaxAge" value="30" />
			<property name="maxConnectionsPerPartition" value="30" />
			<property name="minConnectionsPerPartition" value="10" />
			<property name="partitionCount" value="3" />
			<property name="acquireIncrement" value="5" />
			<property name="statementsCacheSize" value="100" />
			<property name="releaseHelperThreads" value="3" />
	    </dataSource>     
<!-- JNDI连接池，推荐在正式发布时使用 -->
<!--         <dataSource type="JNDI"> -->
<!--              <property name = "data_source" value= "${data_source}"/> -->
<!--         </dataSource> -->
     </environment>
  </environments>
	<mappers>
     <mapper resource="com/hdsx/jxzhpt/xtgl/config/xtgl.xml"/>
	<!-- qwh begin -->
	<mapper resource="com/hdsx/jxzhpt/gcgl/config/gcglwqgz.xml"/>
	<!-- qwh end -->
	<!-- lhp -->
	<mapper resource="com/hdsx/jxzhpt/lwxm/xmjck/config/jckabgc.xml"/>
	<!-- lhp -->
	<!-- rh -->
	<mapper resource="com/hdsx/jxzhpt/jhgl/config/Plan_gcgj.xml"/>
	<!-- rh -->
  </mappers>
</configuration>
