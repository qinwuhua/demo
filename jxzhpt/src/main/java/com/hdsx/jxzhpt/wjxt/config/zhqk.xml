	<select id="selectZhqkList" parameterType="zhqk" resultType="zhqk">
		select * from (
		select m.*,rownum rn from (
			select * from ZHSSQK
			where 1=1 
		<if test="gydw != null and  gydw != ''">
			and gydw like '%'||#{jhid}||'%'
		</if>
		<if test="kssj != null and  kssj != ''">
			and sbrq >= #{kssj}
		</if>
		<if test="jssj != null and  jssj != ''">
			<![CDATA[and sbrq <= #{jssj}]]>
		</if>
		order by sbrq desc
		)  m
			where <![CDATA[rownum<=(#{page} * #{rows})]]>
		) 
			where RN>((#{page} - 1) * #{rows})
		
	</select>
