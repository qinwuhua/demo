<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"mybatis-3-config.dtd">
<configuration>
  <properties resource="dao.properties"/>
  <typeAliases>
     <!-- BONECP数据库连接池 -->
     <typeAlias type="com.hdsx.dao.util.BoneCPFactory" alias="BONECP"/>
     <typeAlias type="com.hdsx.jxcsxm.xtgl.bean.TreeNode" alias="treenode"/>
     <typeAlias type="com.hdsx.jxcsxm.xtgl.bean.Master" alias="master"/>
     <typeAlias type="com.hdsx.jxcsxm.xtgl.bean.Unit" alias="unit"/>
     <typeAlias type="com.hdsx.jxcsxm.xtgl.bean.Param" alias="param"/>
     <typeAlias type="com.hdsx.jxcsxm.xtgl.bean.Dzdt" alias="dzdt"/>
     <typeAlias type="com.hdsx.jxcsxm.xtgl.bean.ProgBean" alias="pb"/>
	 <!-- qwh -->
	 <typeAlias type="com.hdsx.jxcsxm.xtgl.bean.Xmjbxx" alias="xmjbxx"/>
	 <typeAlias type="com.hdsx.jxcsxm.zjdw.bean.XmZjdw" alias="xmzjdw"/>
	 <typeAlias type="com.hdsx.jxcsxm.zjbf.bean.XmZjbf" alias="xmzjbf"/>
     <typeAlias type="com.hdsx.jxcsxm.tjbb.bean.Excel_list" alias="elist"/>
     <typeAlias type="com.hdsx.jxcsxm.zcgl.bean.Zcgl" alias="zcgl"/>
  </typeAliases>
  <environments default="jdbc">
     <environment id="jdbc">
       <transactionManager type="JDBC"/>
       <!-- BONECP连接池，推荐在开发时使用 -->
       <dataSource type="BONECP">
			<property name="driverClass" value="${driverClass}" />
			<property name="jdbcUrl" value="${jdbcUrl}" />
			<property name="username" value="${username}" />
			<property name="password" value="${password}" />
			<property name="idleConnectionTestPeriod" value="60" />
			<property name="idleMaxAge" value="30" />
			<property name="maxConnectionsPerPartition" value="30" />
			<property name="minConnectionsPerPartition" value="10" />
			<property name="partitionCount" value="3" />
			<property name="acquireIncrement" value="5" />
			<property name="statementsCacheSize" value="100" />
			<property name="releaseHelperThreads" value="3" />
	    </dataSource>     
<!-- JNDI连接池，推荐在正式发布时使用 -->
<!--         <dataSource type="JNDI"> -->
<!--              <property name = "data_source" value= "${data_source}"/> -->
<!--         </dataSource> -->
     </environment>
     
     <environment id="jdbc_jh">
       <transactionManager type="JDBC"/>
       <!-- BONECP连接池，推荐在开发时使用 -->
       <dataSource type="BONECP">
			<property name="driverClass" value="${driverClass_jh}" />
			<property name="jdbcUrl" value="${jdbcUrl_jh}" />
			<property name="username" value="${username_jh}" />
			<property name="password" value="${password_jh}" />
			<property name="idleConnectionTestPeriod" value="60" />
			<property name="idleMaxAge" value="30" />
			<property name="maxConnectionsPerPartition" value="30" />
			<property name="minConnectionsPerPartition" value="10" />
			<property name="partitionCount" value="3" />
			<property name="acquireIncrement" value="5" />
			<property name="statementsCacheSize" value="100" />
			<property name="releaseHelperThreads" value="3" />
	    </dataSource>     
<!-- JNDI连接池，推荐在正式发布时使用 -->
<!--         <dataSource type="JNDI"> -->
<!--              <property name = "data_source" value= "${data_source}"/> -->
<!--         </dataSource> -->
     </environment>
  </environments>
	<mappers>
     <mapper resource="com/hdsx/jxcsxm/xtgl/config/xtgl.xml"/>
     <mapper resource="com/hdsx/jxcsxm/xtgl/config/dzdt.xml"/>
	 <!-- qwh -->
	 <mapper resource="com/hdsx/jxcsxm/jhcx/config/jhcx.xml"/>
	 <mapper resource="com/hdsx/jxcsxm/zjdw/config/zjdw.xml"/>
	 <mapper resource="com/hdsx/jxcsxm/zjbf/config/zjbf.xml"/>
	 <mapper resource="com/hdsx/jxcsxm/tjbb/config/tjbb.xml"/>
	 <mapper resource="com/hdsx/jxcsxm/zcgl/config/zcgl.xml"/>
  </mappers>
</configuration>
